{
  "name": "photo-process",
  "version": "0.0.1",
  "description": "Processes photos",
  "main": "index.js",
  "type": "commonjs",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc",
    "start": "node build/index.js",
    "dev": "tsc && node build/index.js",
    "server:start": "pkill -f \"node.*build/index.js\" 2>/dev/null || true && nohup node build/index.js > server-debug.log 2>&1 & echo \"Server PID: $!\"",
    "server:stop": "pkill -f \"node.*build/index.js\"",
    "server:status": "curl -s http://localhost:9000/api/persons/unidentified?limit=1 > /dev/null && echo \"✅ Server responding\" || echo \"❌ Server not responding\"",
    "db:migrate": "./tools/database/migrate.sh",
    "db:seed": "./tools/database/seed.sh",
    "db:create-migration": "./tools/database/create-migration.sh",
    "db:create-seed": "./tools/database/create-seed.sh",
    "db:start": "./tools/database/database.sh",
    "maintenance:fix-dates": "node tools/maintenance/fix-dates.js",
    "maintenance:migrate-features": "node tools/maintenance/migrate-features.js",
    "maintenance:retroactive": "node tools/maintenance/retroactive-process.js",
    "maintenance:update-objects": "node tools/maintenance/update-objects.js",
    "maintenance:import-faces": "node tools/maintenance/import-faces.js",
    "maintenance:import-missing": "node tools/maintenance/import-missing.js",
    "maintenance:check-missing": "node tools/maintenance/check-missing.js",
    "cleanup:menu": "node tools/cleanup/cleanup-menu.js",
    "cleanup:compreface": "node tools/cleanup/cleanup-compreface.js",
    "cleanup:fresh-start": "node tools/cleanup/cleanup-fresh-start.js",
    "cleanup:local-data": "node tools/cleanup/cleanup-local-data.js",
    "cleanup:low-confidence": "node tools/cleanup/cleanup-low-confidence.js",
    "test:full-processing": "node tools/testing/test-full-processing.js",
    "test:object-detection": "node tools/testing/test-object-detection.js",
    "test:single-file": "node tools/testing/test-single-file.js"
  },
  "author": "Stephen Young <stephen@theyoungs.cloud>",
  "license": "ISC",
  "dependencies": {
    "@tensorflow-models/coco-ssd": "^2.2.3",
    "@tensorflow/tfjs-node": "^4.22.0",
    "color": "^5.0.0",
    "dotenv": "^16.5.0",
    "exiftool-vendored": "^29.3.0",
    "express": "^5.1.0",
    "form-data": "^4.0.2",
    "knex": "^3.1.0",
    "mime-types": "^3.0.1",
    "mysql": "^2.18.1",
    "mysql2": "^3.14.1",
    "node-fetch": "^2.7.0",
    "nodemon": "^3.1.10",
    "sharp": "^0.34.1"
  },
  "devDependencies": {
    "@types/express": "^5.0.2",
    "@types/mime-types": "^2.1.4",
    "@types/node": "^22.15.31",
    "@types/node-fetch": "^2.6.12",
    "typescript": "^5.8.3"
  }
}
